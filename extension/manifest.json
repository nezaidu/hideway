{
  "manifest_version": 2,
  "name": "hideway",
  "version": "0.0.0",
  "permissions": ["contextMenus", "storage"],
  
  "background": {
    "scripts": [
      "background/socket.io.js",
      "background/watcher.js",
      "background/context-menu.js",
      "background/store.js"
    ] 
  },
  
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },

  "content_scripts": [{
    "js": ["content/test.js"],
    "css": ["content/test.css"],
    "run_at": "document_end",
    "matches": ["*://*/*"]
  }]
}